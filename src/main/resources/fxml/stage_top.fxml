<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="stageTopParent" prefHeight="30.0" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.jailbreakers.ui.stagetop.StageTopController">
   <padding>
      <Insets left="5.0" right="5.0" />
   </padding>
   <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="1.0" prefHeight="30.0" spacing="10.0" AnchorPane.leftAnchor="0.0">
      <HBox fx:id="titleParent" prefHeight="30.0" spacing="10.0">
         <ImageView fitHeight="29.0" fitWidth="80.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@../images/icon.png" />
            </image>
         </ImageView>
         <Label text="Diary - JailBreakers">
            <font>
               <Font size="18.0" />
            </font>
         </Label>
      </HBox>
      <Region fx:id="space" prefHeight="30.0" prefWidth="200.0" />
      <HBox fx:id="windowEventParent" prefHeight="30.0" spacing="10.0">
         <FontAwesomeIconView fx:id="minimize" glyphName="WINDOW_MINIMIZE" size="30" />
         <FontAwesomeIconView fx:id="close" glyphName="CLOSE" size="30" />
      </HBox>
   </HBox>
</AnchorPane>
