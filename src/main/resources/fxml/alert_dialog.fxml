<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="150.0" prefWidth="250.0" spacing="10.0" type="VBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="title">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="message" alignment="TOP_LEFT" wrapText="true" />
      <HBox alignment="CENTER_RIGHT">
         <children>
            <JFXButton fx:id="neutralButton" visible="false" />
            <JFXButton fx:id="negativeButton" visible="false" />
            <JFXButton fx:id="positiveButton" visible="false" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</fx:root>
